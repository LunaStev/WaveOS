import("iosys");

fun setup_memory() {
    println("Setting up memory...");
}

fun allocate_memory(size :i32) -> ptr {
    var allocated_ptr :ptr = 0x1000;
    println("Allocating " + size + " bytes of memory.");
    return allocated_ptr;
}

fun free_memory(ptr :ptr) {
    println("Freeing memory at address " + ptr);
}